<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.RB.rebasejumpers">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/main_logo"
        android:label="Where&apos;s My Stuff?"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name="com.example.RB.rebasejumpers.LoginActivity"
            android:label="Where&apos;s My Stuff?">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.RB.rebasejumpers.RegistrationActivity1"
            android:label="@string/title_activity_registration1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.RB.rebasejumpers.LogoutScreen"
            android:label="@string/title_activity_logout__screen"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="com.example.RB.rebasejumpers.ItemView"
            android:label="Item List"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.RB.rebasejumpers.NewItemActivity"
            android:label="New Item"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.example.RB.rebasejumpers.BannedUnbanned"
            android:label="@string/title_activity_banned__unbanned"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>
